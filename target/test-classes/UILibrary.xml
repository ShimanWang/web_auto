<?xml version="1.0" encoding="UTF-8"?>
<!-- pages下面有多个page -->
<pages>
    <!-- 一个page元素代表一个注册页面 -->
    <page keyword="registerPage">
        <uiElement keyword="mobileInput" by="id" value="mobilephone"></uiElement>
        <uiElement keyword="pwdInput" by="name" value="password"></uiElement>
        <uiElement keyword="confirmPwdInput" by="id" value="pwdconfirm"></uiElement>
        <uiElement keyword="verifyCodeInput" by="id" value="verifycode"></uiElement>
        <uiElement keyword="signUpButton" by="id" value="signup-button"></uiElement>
        <uiElement keyword="tipsText" by="className" value="tips"></uiElement>

    </page>

    <!-- 一个page元素代表一个登陆页面 -->
    <page keyword="loginPage">
        <uiElement keyword="mobileInput" by="id" value="mobilephone"></uiElement>
        <uiElement keyword="pwdInput" by="id" value="password"></uiElement>
        <uiElement keyword="loginButton" by="id" value="login"></uiElement>
        <uiElement keyword="tipsText" by="className" value="tips"></uiElement>

    </page>

</pages>